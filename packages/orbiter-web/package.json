{
  "name": "orbiter-web",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "concurrently --names astro,api --prefix-colors cyan,magenta \"npm run dev:astro\" \"npm run dev:api\"",
    "dev:astro": "astro dev",
    "dev:api": "uv run uvicorn orbiter_web.app:app --reload --host 127.0.0.1 --port 8000",
    "build": "astro check && astro build",
    "preview": "astro preview",
    "check": "astro check"
  },
  "dependencies": {
    "@fontsource/bricolage-grotesque": "^5.2.10",
    "@tailwindcss/vite": "^4.1.18",
    "astro": "^5.0.0",
    "clsx": "^2.1.1",
    "tailwind-merge": "^3.4.1",
    "tailwindcss": "^4.1.18"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.0",
    "concurrently": "^9.2.1",
    "typescript": "^5.7.0"
  }
}
